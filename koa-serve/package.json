{
  "name": "node-react",
  "version": "1.0.0",
  "description": "node-decorator",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src  --out-dir dist",
    "dev": "set NODE_ENV=development &&  supervisor  -i ./dist --watch src dev.js",
    "start": "npm run build && set NODE_ENV=production  && supervisor --watch dist production",
    "pm2": "pm2 start production.js --name 'node-react' --env  NODE_ENV='production' --output ./logs/logs-out.log  --error ./logs/logs-error.log  --watch dist"
  },
  "keywords": [
    "node",
    "decorator",
    "node-decorator"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-register": "^6.26.0",
    "supervisor": "^0.12.0"
  },
  "dependencies": {
    "formidable": "^1.2.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "mysql": "^2.17.1"
  }
}
